        <style>
            body{background:rgb(29, 55, 86)}.content{opacity:0;transition:opacity 0.5s;} .tab-1{display:none}
            #newsbox{
                text-align:center;
            }
            .post-row{
                display:flex;
                justify-content:center;
                align-items:end;
                padding:20px;
                max-width:1100px;
            }
            .post{
                background:white;
                border-radius:15px;
                max-width:500px;
                color:black;
                margin-left:20px;
                flex-basis: 50%;
                position:relative;
            }
            .post:first-child{margin:0;}
            .post > .title{
                padding:15px;
                text-align:center;
            }
            .carousel{
                display:flex;
                margin-left:0;
                transition:margin-left 1s;
                background:#222;
            }
            .post > .media > .carousel > img,
            .post > .media > img,
            .post > .media > video{
                outline:none;
                width:100%;
            }
            .post-overlay{
                position:absolute;
                z-index:0;
                top:0;
                left:100000%;
                width:95%;
                height:100%;
                display:flex;
                align-items:center;
                justify-content:center;
                transition:left 1s;
                pointer-events:none;
                /*background:rgba(0,0,255,0.5);*/
            }
            .post-overlay-inner{
                width:100%;
                pointer-events:none;
                /*background:green;*/
                display:flex;
                justify-content: space-between;
            }
            .post-overlay-inner>img{
                pointer-events:auto;
                display:none;
            }
            .post-overlay-inner>img:first-of-type {
              transform: rotate(180deg);
            }
            .post-overlay-inner>img:last-of-type {
              margin-left: auto;
            }
            @media only screen and (max-width: 810px){
                .post-row{
                    display:block;
                    margin:0;
                    margin-bottom:20px;
                    padding:0;
                }
                .post{
                    margin:0;
                    margin-top:20px;
                }
            }
            @media only screen and (max-width: 650px){
                #newsbox{
                    width:100%;
                    padding-left:10%;
                }
                .post-row{
                    width:90%;
                }
                .post{width:100%;}
            }
            @media only screen and (max-width: 575px) {
                .livepromo {
                    font-size: 20px;
                }
            }
            @media only screen and (max-width: 360px) {
              .livepromo {
                font-size: 15px;
              }
            }
            @media only screen and (min-width: 576px) {
                .livepromo {
                    font-size: 30px;
                }
            }
            @media only screen and (min-width: 768px) {
                .livepromo {
                    font-size: 20px;
                }
            }
            @media only screen and (min-width: 992px) {
                .livepromo {
                    font-size: 30px;
                }
            }
            @media only screen and (min-width: 1583px) {
                .livepromo {
                    font-size: 25px;
                }
            }
        </style>
